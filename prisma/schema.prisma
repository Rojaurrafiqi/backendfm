model obat {
  id                   Int              @id @default(autoincrement())
  nama_obat            String?
  kategori_obat        String?
  deskripsi_obat       String?
  dosis_obat           String?
  efek_samping         String?
  peringatan           String?
  instruksi_penggunaan String?
  createdAt            DateTime?        @default(now())
  updatedAt            DateTime?        @updatedAt
  penjualan_obat       penjualan_obat[]
  resep                resep[]
  resep_umum           resep_umum[]
  stok_obat            stok_obat[]
}

// tidak digunakan lagi
model stok_obat {
  id                 Int       @id @default(autoincrement())
  id_obat            Int?
  jumlah_stok        String?
  tanggal_kadaluarsa String?
  tanggal_penerimaan String?
  lokasi_penyimpanan String?
  batas_minimum_stok String?
  status_stok        String?
  catatan            String?
  createdAt          DateTime? @default(now())
  updatedAt          DateTime? @updatedAt
  harga_beli         String?
  harga_jual         String?
  obat_data          obat?     @relation(fields: [id_obat], references: [id], onDelete: Cascade)

  @@index([id_obat], map: "stok_obat_id_obat_fkey")
}

// tidak digunakan lagi
model penjualan_obat {
  id                   Int       @id @default(autoincrement())
  id_obat              Int?
  jumlah               String?
  harga_satuan         String?
  diskon               String?
  total_harga          String?
  tanggal_penjualan    String?
  informasi_pembayaran String?
  metode_pembayaran    String?
  createdAt            DateTime? @default(now())
  updatedAt            DateTime? @updatedAt
  obat_data            obat?     @relation(fields: [id_obat], references: [id], onDelete: Cascade)

  @@index([id_obat], map: "penjualan_obat_id_obat_fkey")
}

// tidak digunakan lagi
model resep {
  id                   Int                 @id @default(autoincrement())
  id_obat              Int
  id_pasien_rm         Int?
  id_dokter            Int?
  tanggal_resep        String?
  dosis                String?
  frekuensi            String?
  instruksi_penggunaan String?
  durasi_obat          String?
  jumlah               String?
  status_resep         String?
  createdAt            DateTime?           @default(now())
  updatedAt            DateTime?           @updatedAt
  dokter               dokter?             @relation(fields: [id_dokter], references: [id])
  obat_data            obat                @relation(fields: [id_obat], references: [id], onDelete: Cascade)
  pasien_rm            rekam_medis_pasien? @relation(fields: [id_pasien_rm], references: [id])

  @@index([id_dokter], map: "resep_id_dokter_fkey")
  @@index([id_obat], map: "resep_id_obat_fkey")
  @@index([id_pasien_rm], map: "resep_id_pasien_rm_fkey")
}

model resep_umum {
  id                   Int       @id @default(autoincrement())
  id_obat              Int
  nama_dokter          String?
  nama_pasien          String?
  tanggal_resep        String?
  dosis                String?
  frekuensi            String?
  instruksi_penggunaan String?
  durasi_obat          String?
  jumlah               String?
  status_resep         String?
  createdAt            DateTime? @default(now())
  updatedAt            DateTime? @updatedAt
  obat_data            obat      @relation(fields: [id_obat], references: [id], onDelete: Cascade)

  @@index([id_obat], map: "resep_umum_id_obat_fkey")
}

model pasien_ralan {
  id                       Int                 @id @default(autoincrement())
  no_registrasi            String?
  id_pasien_rm             Int?
  id_pembayaran            Int?
  createdAt                DateTime?           @default(now())
  updatedAt                DateTime?           @updatedAt
  jenis_konsultasi         String?
  jenis_pasien             String?
  no_antrian               String?
  tipe_pasienId            Int?
  catatan                  String?
  deposit                  String?
  asal_rujukan             String?
  cara_daftar              String?
  isCheckout_Poli          Int?                @default(0)
  dokter                   String?
  id_sumber_pasien         String?
  isKasir                  String?             @default("0")
  isResep                  String?             @default("0")
  jam_checkout             String?
  ket_kondisi_masuk        String?
  poliklinik               String?
  sts_pasien_checkout_poli String?
  pasien_rm                rekam_medis_pasien? @relation(fields: [id_pasien_rm], references: [id])
  jenis_pembayaran         pembayaran?         @relation(fields: [id_pembayaran], references: [id])
  tipe_pasien              tipe_pasien?        @relation(fields: [tipe_pasienId], references: [id])

  @@index([id_pasien_rm], map: "pasien_ralan_id_pasien_rm_fkey")
  @@index([id_pembayaran], map: "pasien_ralan_id_pembayaran_fkey")
  @@index([tipe_pasienId], map: "pasien_ralan_tipe_pasienId_fkey")
}

model poliklinik {
  id                Int                 @id @default(autoincrement())
  kode_ruang        Int?
  nama_poliklinik   String?
  createdAt         DateTime?           @default(now())
  updatedAt         DateTime?           @updatedAt
  jadwal_poli       jadwal_poli[]
  jadwal_poliklinik jadwal_poliklinik[]
}

// tidak digunakan lagi krna jadwal dokter sering berubah
model jadwal_poliklinik {
  id              Int         @id @default(autoincrement())
  id_poli         Int?
  id_hari         Int?
  id_dokter       Int?
  jam             String?
  createdAt       DateTime?   @default(now())
  updatedAt       DateTime?   @updatedAt
  catatan         String?
  dokter_data     dokter?     @relation(fields: [id_dokter], references: [id])
  hari            nama_hari?  @relation(fields: [id_hari], references: [id])
  poliklinik_data poliklinik? @relation(fields: [id_poli], references: [id])

  @@index([id_dokter], map: "jadwal_poliklinik_id_dokter_fkey")
  @@index([id_hari], map: "jadwal_poliklinik_id_hari_fkey")
  @@index([id_poli], map: "jadwal_poliklinik_id_poli_fkey")
}

// tidak digunakan lagi, karena jadwal dokter berubah
model jadwal_poli {
  id              Int         @id @default(autoincrement())
  createdAt       DateTime?   @default(now())
  updatedAt       DateTime?   @updatedAt
  id_dokter       Int?
  id_hari         Int?
  id_poli         Int?
  dokter_data     dokter?     @relation(fields: [id_dokter], references: [id])
  hari            nama_hari?  @relation(fields: [id_hari], references: [id])
  poliklinik_data poliklinik? @relation(fields: [id_poli], references: [id])

  @@index([id_dokter], map: "jadwal_poli_id_dokter_fkey")
  @@index([id_hari], map: "jadwal_poli_id_hari_fkey")
  @@index([id_poli], map: "jadwal_poli_id_poli_fkey")
}

model pembayaran {
  id               Int            @id @default(autoincrement())
  jenis_pembayaran String?
  createdAt        DateTime?      @default(now())
  updatedAt        DateTime?      @updatedAt
  pasien_ralan     pasien_ralan[]
}

model tipe_pasien {
  id                Int            @id @default(autoincrement())
  jenis_tipe_pasien String?
  createdAt         DateTime?      @default(now())
  updatedAt         DateTime?      @updatedAt
  pasien_ralan      pasien_ralan[]
}

model nama_hari {
  id                Int                 @id @default(autoincrement())
  list_nama_hari    String?
  createdAt         DateTime?           @default(now())
  updatedAt         DateTime?           @updatedAt
  jadwal_poli       jadwal_poli[]
  jadwal_poliklinik jadwal_poliklinik[]
}

model users {
  id            Int       @id @default(autoincrement())
  email         String?
  password      String?
  jabatan       String?
  status        String?
  refresh_token String?
  createdAt     DateTime? @default(now())
  updatedAt     DateTime? @updatedAt
  name          String?
  jenis_kelamin String?
  id_finger     String?
  dokter        dokter[]
}

model jabatan {
  id           Int       @id @default(autoincrement())
  nama_jabatan String?
  createdAt    DateTime? @default(now())
  updatedAt    DateTime? @updatedAt
}

model dokter {
  id                Int                 @id @default(autoincrement())
  nama_dokter       String?
  jenis_kelamin     String?
  tanggal_lahir     String?
  spesialis         String?
  nomor_hp          String?
  alamat            String?
  email             String?
  createdAt         DateTime?           @default(now())
  updatedAt         DateTime?           @updatedAt
  sub_spesialis     String?
  tempat_lahir      String?
  id_user           Int?
