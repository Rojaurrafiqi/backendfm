generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model rekam_medis_pasien {
  id                        Int            @id @default(autoincrement())
  id_finger                 String?        @map("id_finger")
  foto                      String?        @map("foto")
  tanda_tangan              String?        @map("tanda_tangan")
  no_mr                     String         @map("no_mr")
  no_mr_lama                String?        @map("no_mr_lama")
  kode_dpjp                 String?        @map("kode_dpjp")
  gelar_dpn                 String?        @map("gelar_dpn")
  nama_user                 String?        @map("nama_user")
  gelar_blk                 String?        @map("gelar_blk")
  id_shift                  Int?           @map("id_shift")
  tmp_lhr                   String?        @map("tmp_lhr")
  tgl_lhr                   String?        @map("tgl_lhr")
  gender_data               gender?        @relation(fields: [id_jk], references: [id_gender])
  id_jk                     Int?           @map("id_jk")
  id_kawin                  Int?           @map("id_kawin")
  id_agama                  String?        @map("id_agama")
  gol_darah                 String?        @map("gol_darah")
  berat_lahir               String?        @map("berat_lahir")
  id_jpdd                   Int?           @map("id_jpdd")
  id_prodi                  Int?           @map("id_prodi")
  id_pekerjaan              Int?
  id_pekerjaan_sts          Int?           @map("id_pekerjaan_sts")
  id_kantor                 Int?           @map("id_kantor")
  id_grupsdm                Int?           @map("id_grupsdm")
  tarif_konsul              String?        @map("tarif_konsul")
  sts_tarif_dikelola_dokter String?        @map("sts_tarif_dikelola_dokter")
  ket_spesialisasi          String?        @map("ket_spesialisasi")
  no_sip                    String?        @map("no_sip")
  id_ruangan                Int?           @map("id_ruangan")
  tmt_cpns                  String?        @map("tmt_cpns")
  tmt_pns                   String?        @map("tmt_pns")
  nip                       String?        @map("nip")
  no_karpeg                 String?        @map("no_karpeg")
  npwp                      String?        @map("npwp")
  id_desa                   Int?           @map("id_desa")
  alamat                    String?        @map("alamat")
  jenis_identitas           String?        @map("jenis_identitas")
  no_identitas              String?        @map("no_identitas")
  no_kk                     String?        @map("no_kk")
  no_hp                     String?        @map("no_hp")
  email                     String?        @map("email")
  grup_user                 Int?           @map("grup_user")
  sts_user                  Int?           @map("sts_user")
  sts_prioritas             String?        @map("sts_prioritas")
  id_prov                   Int?           @map("id_prov")
  id_kabkota                Int?           @map("id_kabkota")
  id_kecamatan              Int?           @map("id_kecamatan")
  id_kelp_sdm               Int?           @map("id_kelp_sdm")
  nip_lama                  String?        @map("nip_lama")
  cuti_tahunan              String?        @map("cuti_tahunan")
  tmt_pensiun               String?        @map("tmt_pensiun")
  sts_allowed_credit        String?        @map("sts_allowed_credit")
  riwayat_alergi_obat       String?        @map("riwayat_alergi_obat")
  riwayat_alergi_makanan    String?        @map("riwayat_alergi_makanan")
  idcardprinted             String?        @map("idcardprinted")
  idcardtaked               String?        @map("idcardtaked")
  idcardpaid                String?        @map("idcardpaid")
  idcardexpired             String?        @map("idcardexpired")
  date_registered           String?        @map("date_registered")
  user_registered           String?        @map("user_registered")
  date_modified             String?        @map("date_modified")
  user_modified             String?        @map("user_modified")
  isdefault                 String?        @map("isdefault")
  password                  String?        @map("password")
  sts_update                String?        @map("sts_update")
  bill                      bill[]
  jual_barang               jual_barang[]
  kasir                     kasir[]
  pasien_igd                pasien_igd[]
  dokterRalan               pasien_ralan[] @relation("DokterRalan")
  pasienRalan               pasien_ralan[] @relation("PasienRalan")
  pasien_ranap              pasien_ranap[]
  users                     users[]
}

model pekerjaan {
  id             Int     @id @default(autoincrement())
  nama_pekerjaan String?
}

model agama {
  id         Int     @id @default(autoincrement())
  list_agama String?
}

model status_kawin {
  id                Int     @id @default(autoincrement())
  list_status_kawin String?
}

model pendidikan {
  id              Int     @id @default(autoincrement())
  list_pendidikan String?
}

model golongan_darah {
  id                  Int     @id @default(autoincrement())
  list_golongan_darah String?
}

model pasien_igd {
  id                                              Int                                               @id @default(autoincrement())
  id_pasien_rm                                    Int?
  tgl_masuk                                       String?
  jam_masuk                                       String?
  cara_masuk                                      String?
  asal_rujukan                                    String?
  alasan_rujukan                                  String?
  dokter_merujuk                                  String?
  kondisi_keluar_igd                              String?
  tanggal_keluar_igd                              String?
  jam_keluar_igd                                  String?
  pembayaran_igd                                  String?
  published                                       Boolean?                                          @default(false)
  createdAt                                       DateTime?                                         @default(now())
  updatedAt                                       DateTime?
  status                                          String?
  asesmen_awal_pasien_igd                         asesmen_awal_pasien_igd[]
  obat_pasien_igd                                 obat_pasien_igd[]
  pasien_rm                                       rekam_medis_pasien?                               @relation(fields: [id_pasien_rm], references: [id])
  tindakan_igd                                    tindakan_igd[]
  triase_anamnesa_pasien_igd                      triase_anamnesa_pasien_igd[]
  triase_ats_pasien_igd                           triase_ats_pasien_igd[]
  triase_kondisi_terakhir_pasien_igd              triase_kondisi_terakhir_pasien_igd[]
  triase_pasien_igd                               triase_pasien_igd[]
  triase_pemeriksaan_fisik_pasien_igd             triase_pemeriksaan_fisik_pasien_igd[]
  triase_pemeriksaan_penunjang_pasien_igd         triase_pemeriksaan_penunjang_pasien_igd[]
  triase_permasalahan_yang_dikonsulkan_pasien_igd triase_permasalahan_yang_dikonsulkan_pasien_igd[]
  triase_planning_pasien_igd                      triase_planning_pasien_igd[]
  triase_tanda_vital_pasien_igd                   triase_tanda_vital_pasien_igd[]

  @@index([id_pasien_rm], map: "pasien_igd_id_pasien_rm_fkey")
}

model tindakan_igd {
  id            Int        @id @default(autoincrement())
  id_pasien_igd Int
  tindakan      String?
  jam_tindakan  String?
  catatan       String?
  createdAt     DateTime?  @default(now())
  updatedAt     DateTime?  @updatedAt
  pasien_igd    pasien_igd @relation(fields: [id_pasien_igd], references: [id], onDelete: Cascade)

  @@index([id_pasien_igd], map: "tindakan_igd_id_pasien_igd_fkey")
}

model list_tindakan_igd {
  id        Int       @id @default(autoincrement())
  tindakan  String?
  createdAt DateTime? @default(now())
  updatedAt DateTime? @updatedAt
}

model obat_pasien_igd {
  id            Int        @id @default(autoincrement())
  id_pasien_igd Int
  obat          String?
  quantity      String?
  catatan       String?
  createdAt     DateTime?  @default(now())
  updatedAt     DateTime?  @updatedAt
  pasien_igd    pasien_igd @relation(fields: [id_pasien_igd], references: [id], onDelete: Cascade)

  @@index([id_pasien_igd], map: "obat_pasien_igd_id_pasien_igd_fkey")
}

model list_obat_pasien_igd {
  id        Int       @id @default(autoincrement())
  list_obat String?
  createdAt DateTime? @default(now())
  updatedAt DateTime? @updatedAt
}

model triase_pasien_igd {
  id            Int        @id @default(autoincrement())
  keluhan_utama String?
  id_pasien_igd Int
  createdAt     DateTime?  @default(now())
  updatedAt     DateTime?  @updatedAt
  pasien_igd    pasien_igd @relation(fields: [id_pasien_igd], references: [id], onDelete: Cascade)

  @@index([id_pasien_igd], map: "triase_pasien_igd_id_pasien_igd_fkey")
}

model triase_tanda_vital_pasien_igd {
  id                     Int        @id @default(autoincrement())
  tekanan_darah          String?
  frekuensi_nadi         String?
  frekuensi_nafas        String?
  suhu                   String?
  sat_o2                 String?
  riwayat_alergi_makanan String?
  riwayat_alergi_obat    String?
  riwayat_alergi_lainya  String?
  berat_badan            String?
  tinggi_badan           String?
  id_pasien_igd          Int
  createdAt              DateTime?  @default(now())
  updatedAt              DateTime?  @updatedAt
  pasien_igd             pasien_igd @relation(fields: [id_pasien_igd], references: [id], onDelete: Cascade)

  @@index([id_pasien_igd], map: "triase_tanda_vital_pasien_igd_id_pasien_igd_fkey")
}

model triase_anamnesa_pasien_igd {
  id                          Int        @id @default(autoincrement())
  keluhan_utama               String?
  riwayat_perjalanan_penyakit String?
  riwayat_penyakit_terdahulu  String?
  riwayat_pemakaian_obat      String?
  riwayat_penyakit_keluarga   String?
  id_pasien_igd               Int
  createdAt                   DateTime?  @default(now())
  updatedAt                   DateTime?  @updatedAt
  pasien_igd                  pasien_igd @relation(fields: [id_pasien_igd], references: [id], onDelete: Cascade)

  @@index([id_pasien_igd], map: "triase_anamnesa_pasien_igd_id_pasien_igd_fkey")
}

model triase_pemeriksaan_fisik_pasien_igd {
  id               Int        @id @default(autoincrement())
  mata             String?
  telinga          String?
  hidung           String?
  mulut            String?
  tenggorokan      String?
  leher            String?
  paru             String?
  jantung          String?
  abdomen          String?
  kandungan        String?
  kemaluan         String?
  exstremitas      String?
  status_lokalis   String?
  pemeriksaan_gigi String?
  id_pasien_igd    Int
  createdAt        DateTime?  @default(now())
  updatedAt        DateTime?  @updatedAt
  pasien_igd       pasien_igd @relation(fields: [id_pasien_igd], references: [id], onDelete: Cascade)

  @@index([id_pasien_igd], map: "triase_pemeriksaan_fisik_pasien_igd_id_pasien_igd_fkey")
}

model triase_pemeriksaan_penunjang_pasien_igd {
  id                 Int        @id @default(autoincrement())
  jenis_pemeriksaan  String?
  diagnosa_sementara String?
  diagnosa_tambahan  String?
  id_pasien_igd      Int
  createdAt          DateTime?  @default(now())
  updatedAt          DateTime?  @updatedAt
  pasien_igd         pasien_igd @relation(fields: [id_pasien_igd], references: [id], onDelete: Cascade)

  @@index([id_pasien_igd], map: "triase_pemeriksaan_penunjang_pasien_igd_id_pasien_igd_fkey")
}

model triase_ats_pasien_igd {
  id                Int        @id @default(autoincrement())
  jalan_nafas       String
  pernafasan_dewasa String?
  pernafasan_anak   String?
  sirkulasi_dewasa  String?
  sirkulasi_anak    String?
  mental_status     String?
  skor_nyeri        String?
  assesment_triase  String?
  plan              String?
  id_pasien_igd     Int
  createdAt         DateTime?  @default(now())
  updatedAt         DateTime?  @updatedAt
  pasien_igd        pasien_igd @relation(fields: [id_pasien_igd], references: [id], onDelete: Cascade)

  @@index([id_pasien_igd], map: "triase_ats_pasien_igd_id_pasien_igd_fkey")
}

model triase_planning_pasien_igd {
  id            Int        @id @default(autoincrement())
  planning      String?
  id_pasien_igd Int
  createdAt     DateTime?  @default(now())
  updatedAt     DateTime?  @updatedAt
  pasien_igd    pasien_igd @relation(fields: [id_pasien_igd], references: [id], onDelete: Cascade)

  @@index([id_pasien_igd], map: "triase_planning_pasien_igd_id_pasien_igd_fkey")
}

model triase_permasalahan_yang_dikonsulkan_pasien_igd {
  id                            Int        @id @default(autoincrement())
  permasalahan_yang_dikonsulkan String?
  id_pasien_igd                 Int
  createdAt                     DateTime?  @default(now())
  updatedAt                     DateTime?  @updatedAt
  pasien_igd                    pasien_igd @relation(fields: [id_pasien_igd], references: [id], onDelete: Cascade)

  @@index([id_pasien_igd], map: "triase_permasalahan_yang_dikonsulkan_pasien_igd_id_pasien_i_fkey")
}

model triase_kondisi_terakhir_pasien_igd {
  id                      Int        @id @default(autoincrement())
  keadaan_umum            String?
  kesadaran               String?
  tekanan_darah           String?
  frekuensi_nadi          String?
  frekuensi_nafas         String?
  temperature             String?
  dokter_penanggung_jawab String?
  id_pasien_igd           Int
  createdAt               DateTime?  @default(now())
  updatedAt               DateTime?  @updatedAt
  pasien_igd              pasien_igd @relation(fields: [id_pasien_igd], references: [id], onDelete: Cascade)

  @@index([id_pasien_igd], map: "triase_kondisi_terakhir_pasien_igd_id_pasien_igd_fkey")
}

model asesmen_awal_pasien_igd {
  id                                                      Int        @id @default(autoincrement())
  kegawatan_pernafasan                                    String?
  kehilangan_tonus_otot                                   String?
  nyeri                                                   String?
  perlambatan_sirkulasi                                   String?
  faktor_pembangkit_gejala_fisik                          String?
  pelayanan_spiritual                                     String?
  perlu_didoakan                                          String?
  bimbingan_rohani                                        String?
  pendampingan_rohani                                     String?
  nama_orang_yang_ingin_dihubungi                         String?
  alamat_orang_yang_ingin_dihubungi                       String?
  hubungan_dengan_pasien                                  String?
  no_telpon_orang_yang_ingin_dihubungi                    String?
  rencana_perawatan_lanjutan                              String?
  ligkungan_disiapkan                                     String?
  perawat_di_rumah                                        String?
  fasilitasi_rs_homecare                                  String?
  asesmen_informasi_reaksi_pasien                         String?
  masalah_keperawatan_reaksi_pasien                       String?
  asesmen_informasi_reaksi_keluarga                       String?
  masalah_keperawatan_reaksi_keluarga                     String?
  dukungan_pelayanan_bagi_pasien                          String?
  kebutuhan_alternative                                   String?
  asesmen_informasi_faktor_resiko_keluarga_ditinggalkan   String?
  masalah_keperawatan_faktor_resiko_keluarga_ditinggalkan String?
  id_pasien_igd                                           Int
  createdAt                                               DateTime?  @default(now())
  updatedAt                                               DateTime?  @updatedAt
  masalah_keperawatan_respon_pasien_terhadap_gejala       String?
  nama_petugas                                            String?
  ttd_petugas                                             String?
  pasien_igd                                              pasien_igd @relation(fields: [id_pasien_igd], references: [id], onDelete: Cascade)

  @@index([id_pasien_igd], map: "asesmen_awal_pasien_igd_id_pasien_igd_fkey")
}

model form_default_igd {
  id        Int       @id @default(autoincrement())
  nama_form String?
  createdAt DateTime? @default(now())
  updatedAt DateTime? @updatedAt
}

model form_default_ranap {
  id        Int       @id @default(autoincrement())
  nama_form String?
  createdAt DateTime? @default(now())
  updatedAt DateTime? @updatedAt
}

model form_default_ralan {
  id        Int       @id @default(autoincrement())
  nama_form String?
  createdAt DateTime? @default(now())
  updatedAt DateTime? @updatedAt
}

model pasien_ranap {
  id               Int                 @id @default(autoincrement())
  id_pasien_rm     Int?
  tanggal_masuk    String?
  tanggal_keluar   String?
  kondisi_keluar   String?
  dokter           String?
  perawat          String?
  createdAt        DateTime?           @default(now())
  updatedAt        DateTime?           @updatedAt
  pembayaran_ranap String?
  no_kamar         String?
  no_bad           String?
  pasien_rm        rekam_medis_pasien? @relation(fields: [id_pasien_rm], references: [id])

  @@index([id_pasien_rm], map: "pasien_ranap_id_pasien_rm_fkey")
}

model kamar_ranap {
  id           Int       @id @default(autoincrement())
  no_kamar     String?
  no_bad       String?
  tipe_kamar   String?
  status_kamar String?
  createdAt    DateTime? @default(now())
  updatedAt    DateTime? @updatedAt
}

model tipe_kamar_ranap {
  id        Int       @id @default(autoincrement())
  tipe      String?
  createdAt DateTime? @default(now())
  updatedAt DateTime? @updatedAt
}

model obat {
  id                   Int              @id @default(autoincrement())
  nama_obat            String?
  kategori_obat        String?
  deskripsi_obat       String?
  dosis_obat           String?
  efek_samping         String?
  peringatan           String?
  instruksi_penggunaan String?
  createdAt            DateTime?        @default(now())
  updatedAt            DateTime?        @updatedAt
  penjualan_obat       penjualan_obat[]
  resep                resep[]
  resep_umum           resep_umum[]
  stok_obat            stok_obat[]
}

model stok_obat {
  id                 Int       @id @default(autoincrement())
  id_obat            Int?
  jumlah_stok        String?
  tanggal_kadaluarsa String?
  tanggal_penerimaan String?
  lokasi_penyimpanan String?
  batas_minimum_stok String?
  status_stok        String?
  catatan            String?
  createdAt          DateTime? @default(now())
  updatedAt          DateTime? @updatedAt
  harga_beli         String?
  harga_jual         String?
  obat_data          obat?     @relation(fields: [id_obat], references: [id], onDelete: Cascade)

  @@index([id_obat], map: "stok_obat_id_obat_fkey")
}

model penjualan_obat {
  id                   Int       @id @default(autoincrement())
  id_obat              Int?
  jumlah               String?
  harga_satuan         String?
  diskon               String?
  total_harga          String?
  tanggal_penjualan    String?
  informasi_pembayaran String?
  metode_pembayaran    String?
  createdAt            DateTime? @default(now())
  updatedAt            DateTime? @updatedAt
  obat_data            obat?     @relation(fields: [id_obat], references: [id], onDelete: Cascade)

  @@index([id_obat], map: "penjualan_obat_id_obat_fkey")
}

model resep {
  id                   Int       @id @default(autoincrement())
  id_obat              Int
  id_pasien_rm         Int?
  id_dokter            Int?
  tanggal_resep        String?
  dosis                String?
  frekuensi            String?
  instruksi_penggunaan String?
  durasi_obat          String?
  jumlah               String?
  status_resep         String?
  createdAt            DateTime? @default(now())
  updatedAt            DateTime? @updatedAt
  dokter               dokter?   @relation(fields: [id_dokter], references: [id])
  obat_data            obat      @relation(fields: [id_obat], references: [id], onDelete: Cascade)

  @@index([id_dokter], map: "resep_id_dokter_fkey")
  @@index([id_obat], map: "resep_id_obat_fkey")
  @@index([id_pasien_rm], map: "resep_id_pasien_rm_fkey")
}

model resep_umum {
  id                   Int       @id @default(autoincrement())
  id_obat              Int
  nama_dokter          String?
  nama_pasien          String?
  tanggal_resep        String?
  dosis                String?
  frekuensi            String?
  instruksi_penggunaan String?
  durasi_obat          String?
  jumlah               String?
  status_resep         String?
  createdAt            DateTime? @default(now())
  updatedAt            DateTime? @updatedAt
  obat_data            obat      @relation(fields: [id_obat], references: [id], onDelete: Cascade)

  @@index([id_obat], map: "resep_umum_id_obat_fkey")
}

model pasien_ralan {
  id                       Int                 @id @default(autoincrement())
  id_pasien_rm             Int?
  id_pembayaran            Int?
  createdAt                DateTime?           @default(now())
  updatedAt                DateTime?           @updatedAt
  jenis_konsultasi         String?
  no_antrian               String?
  catatan                  String?
  asal_rujukan             String?
  cara_daftar              String?
  dokter                   Int?
  id_sumber_pasien         String?
  isKasir                  String?             @default("0")
  isResep                  String?             @default("0")
  jam_checkout             String?
  ket_kondisi_masuk        String?
  poliklinik               Int?
  sts_pasien_checkout_poli String?
  no_registrasi            String?
  isCheckout_Poli          Int?                @default(0)
  asuransi                 Int?
  biaya_adm                String?
  biaya_share_dokter       String?
  no_asuransi              String?
  isBB                     Int?                @default(0) // untuk kode pasien umum sudah bayar atau blm. 1 untuk bayar, 0 untuk blm.
  pembayaranId             Int?
  asuransi_data            tbl_mst_asuransi?   @relation(fields: [asuransi], references: [id_asuransi])
  dokter_data              rekam_medis_pasien? @relation("DokterRalan", fields: [dokter], references: [id])
  pasien_rm                rekam_medis_pasien? @relation("PasienRalan", fields: [id_pasien_rm], references: [id])
  pembayaran               pembayaran?         @relation(fields: [pembayaranId], references: [id])
  poliklinik_data          tbl_mst_ruangan?    @relation(fields: [poliklinik], references: [id_ruangan])

  @@index([asuransi], map: "pasien_ralan_asuransi_fkey")
  @@index([dokter], map: "pasien_ralan_dokter_fkey")
  @@index([id_pasien_rm], map: "pasien_ralan_id_pasien_rm_fkey")
  @@index([pembayaranId], map: "pasien_ralan_pembayaranId_fkey")
  @@index([poliklinik], map: "pasien_ralan_poliklinik_fkey")
}

model poliklinik {
  createdAt    DateTime? @default(now())
  updatedAt    DateTime? @updatedAt
  id_ruangan   Int       @id @default(autoincrement())
  ket_ruangan  String?
  kode_bpjs    String?
  nama_ruangan String?
  no_ruang     Int?
}

model jadwal_poliklinik {
  id          Int        @id @default(autoincrement())
  id_poli     Int?
  id_hari     Int?
  id_dokter   Int?
  jam         String?
  createdAt   DateTime?  @default(now())
  updatedAt   DateTime?  @updatedAt
  catatan     String?
